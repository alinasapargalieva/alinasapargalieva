#include <stdio.h>
#include <locale.h>
#include <math.h>
int main() {
	setlocale(LC_ALL, "Russian");
	float x1, x2, y1, y2, z, b=2;
	puts("Введите х1:");
	scanf_s("%f", &x1);
	puts("Введите х2:");
	scanf_s("%f", &x2);
	puts("Введите y1:");
	scanf_s("%f", &y1);
	puts("Введите y2:");
	scanf_s("%f", &y2);
	z = sqrtf(powf((x2 - x1),b) + powf((y2 - y1),b));
	printf("Расстояние между точками равно %f", z);
}
