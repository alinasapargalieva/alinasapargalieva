#include <stdio.h>
#include <locale.h>
int main() {
    setlocale(LC_ALL, "Russian");
    int n;
	puts("Введите n:");
	scanf_s("%d", &n);
	if (n % 2 == 0) {
		printf("Четное ");
	}
	else {
		printf("Нечетное ");
	}
	if (n>99) {
		printf("трехзначное число");
	}
	 if ((n>9)&(n<100)) {
		printf("двузначное число ");
	}
	 if (n<10) {
		 printf("однозначное число ");
	 }
    return 0;
}
